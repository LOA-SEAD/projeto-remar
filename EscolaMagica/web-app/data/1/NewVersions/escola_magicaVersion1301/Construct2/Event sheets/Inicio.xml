                <event-group collapsed="1" description="Responsavel por ordenar aletoriamente as perguntas que serao exibidas na proxima porta." title="EmbaralhaPerguntas">
                    <sub-events>
                        <variable constant="0" name="PergIndex" static="0" type="number">0</variable>
                        <variable constant="0" name="PergRandom" static="0" type="number">0</variable>
                        <variable comment="Auxiliar na troca de elementos do vetor nas posicoes i e random([0,5])" constant="0" name="ValueAux" static="0" type="number">0</variable>
                        <event-block>
                            <conditions />
                            <actions>
                                <action id="1" name="Set size" type="Perguntas">
                                    <param id="0" name="Width">5</param>
                                    <param id="1" name="Height">1</param>
                                    <param id="2" name="Depth">1</param>
                                </action>
                            </actions>
                        </event-block>
                        <comment>Preenche vetor Perguntas</comment>
                        <event-block>
                            <conditions>
                                <condition id="-10" name="For" type="System">
                                    <param id="0" name="Name">&quot;SetaPerg&quot;</param>
                                    <param id="1" name="Start index">0</param>
                                    <param id="2" name="End index">4</param>
                                </condition>
                            </conditions>
                            <actions>
                                <action id="2" name="Set at X" type="Perguntas">
                                    <param id="0" name="X">PergIndex</param>
                                    <param id="1" name="Value">PergIndex</param>
                                </action>
                                <action id="-10" name="Add to" type="System">
                                    <param id="0" name="Variable">PergIndex</param>
                                    <param id="1" name="Value">1</param>
                                </action>
                            </actions>
                        </event-block>
                        <event-block>
                            <conditions />
                            <actions>
                                <action id="-9" name="Set value" type="System">
                                    <param id="0" name="Variable">PergIndex</param>
                                    <param id="1" name="Value">0</param>
                                </action>
                            </actions>
                        </event-block>
                        <comment>Randomiza valores sem repeti-los</comment>
                        <event-block>
                            <conditions>
                                <condition id="-10" name="For" type="System">
                                    <param id="0" name="Name">&quot;EmbPerg&quot;</param>
                                    <param id="1" name="Start index">0</param>
                                    <param id="2" name="End index">4</param>
                                </condition>
                            </conditions>
                            <actions>
                                <action id="-9" name="Set value" type="System">
                                    <param id="0" name="Variable">PergRandom</param>
                                    <param id="1" name="Value">round(random(4))</param>
                                </action>
                                <action id="-9" name="Set value" type="System">
                                    <param id="0" name="Variable">ValueAux</param>
                                    <param id="1" name="Value">Perguntas.At(PergIndex)</param>
                                </action>
                                <action id="2" name="Set at X" type="Perguntas">
                                    <param id="0" name="X">PergIndex</param>
                                    <param id="1" name="Value">Perguntas.At(PergRandom)</param>
                                </action>
                                <action id="2" name="Set at X" type="Perguntas">
                                    <param id="0" name="X">PergRandom</param>
                                    <param id="1" name="Value">ValueAux</param>
                                </action>
                                <action id="-10" name="Add to" type="System">
                                    <param id="0" name="Variable">PergIndex</param>
                                    <param id="1" name="Value">1</param>
                                </action>
                            </actions>
                        </event-block>
                    </sub-events>
                </event-group>
                <event-block>
                    <conditions>
                        <condition id="-13" name="Pick nth instance" type="System">
                            <param id="0" name="Object">txtCenario</param>
                            <param id="1" name="Instance">0</param>
                        </condition>
                    </conditions>
                    <actions>
                        <action id="0" name="Set text" type="txtCenario">
