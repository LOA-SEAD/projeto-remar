                    <param id="1" name="Value">0</param>
                </action>
                <action id="-9" name="Set value" type="System">
                    <param id="0" name="Variable">NumQuedas</param>
                    <param id="1" name="Value">0</param>
                </action>
                <action id="5" name="Set frame" type="btnEditarSom">
